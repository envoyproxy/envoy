headers_to_add {
  header {
    key: "x-request-start"
    value: "%START_TIME(%s.%3f)%"
  }
  append: { value: true }
}
headers_to_add {
  header {
    key: "x-request-start-f"
    value: "%START_TIME(f)%"
  }
  append: { value: true }
}
headers_to_add {
  header {
    key: "x-request-start-range"
    value: "%START_TIME(%f, %1f, %2f, %3f, %4f, %5f, %6f, %7f, %8f, %9f)%"
  }
  append: { value: true }
}
headers_to_add {
  header {
    key: "x-request-start-default"
    value: "%START_TIME%"
  }
}
request_info {
