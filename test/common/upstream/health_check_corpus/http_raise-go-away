health_check_config {
    timeout {
        seconds: 1
    }
    interval {
        seconds: 1
    }
    unhealthy_threshold {
        value: 2
    }
    healthy_threshold: {
        value: 2
    }
    http_health_check {
        path: "/healthcheck"
        service_name_matcher {
            prefix: "locations"
        }
    }
}
actions {
    respond {
        http_respond {
            headers {
                headers {
                    key: ":status"
                    value: "200"
                }
            }
            status: 200
        }
        tcp_respond {
            
        }
        grpc_respond {
            grpc_respond_headers {
            headers {
                headers {
                    key: ":status"
                    value: "200"
                }
                headers {
                    key: "content-type"
                    value: "application/grpc"
                }
            }
            status: 200
            }
            grpc_respond_bytes {
                status: SERVING
                chunk_size_for_structured_response: 3
            }
            grpc_respond_trailers {
            trailers {
                headers {
                    key: "grpc-status"
                    value: "0"
                }
            }
            }
        }
    }
}
actions {
    raise_go_away: NO_ERROR
}
actions {
    raise_go_away: ERROR
}
