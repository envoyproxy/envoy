  downstream_send_event {
    h2_frames {
      settings {
        flags: NONE
      }
    }
    h2_frames {
      settings {
        flags: ACK
      }
    }
    h2_frames {
      request {
        stream_index: 1
        host: "host"
        path: "/path/to/long/url"
      }
    }
  }
}
